{
  "name": "writenow-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main/index.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron-vite dev",
    "build": "tsc -b && vite build",
    "build:electron": "electron-vite build",
    "lint": "eslint .",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "preview": "vite preview",
    "preview:electron": "electron-vite preview",
    "package": "electron-vite build && electron-builder --config electron-builder.json5",
    "package:win": "electron-vite build && electron-builder --config electron-builder.json5 --win",
    "package:mac": "electron-vite build && electron-builder --config electron-builder.json5 --mac",
    "package:linux": "electron-vite build && electron-builder --config electron-builder.json5 --linux",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm run build:electron && playwright test"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tiptap/core": "^3.17.1",
    "@tiptap/extension-character-count": "^3.17.1",
    "@tiptap/extension-image": "^3.17.1",
    "@tiptap/extension-link": "^3.17.1",
    "@tiptap/extension-placeholder": "^3.17.1",
    "@tiptap/extension-table": "^3.17.1",
    "@tiptap/extension-table-cell": "^3.17.1",
    "@tiptap/extension-table-header": "^3.17.1",
    "@tiptap/extension-table-row": "^3.17.1",
    "@tiptap/extension-task-item": "^3.17.1",
    "@tiptap/extension-task-list": "^3.17.1",
    "@tiptap/extension-text-align": "^3.17.1",
    "@tiptap/extension-underline": "^3.17.1",
    "@tiptap/markdown": "^3.17.1",
    "@tiptap/pm": "^3.17.1",
    "@tiptap/react": "^3.17.1",
    "@tiptap/starter-kit": "^3.17.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "diff": "^5.2.2",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "node-llama-cpp": "^3.15.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "^5.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vscode-jsonrpc": "^8.2.1",
    "vscode-ws-jsonrpc": "^3.5.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/diff": "^7.0.2",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "electron": "^34.5.8",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
