{
  "name": "writenow-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main/index.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron-vite dev",
    "build": "tsc -b && vite build",
    "build:electron": "electron-vite build",
    "lint": "eslint .",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "preview": "vite preview",
    "preview:electron": "electron-vite preview",
    "package": "electron-vite build && electron-builder --config electron-builder.json5",
    "package:win": "electron-vite build && electron-builder --config electron-builder.json5 --win",
    "package:mac": "electron-vite build && electron-builder --config electron-builder.json5 --mac",
    "package:linux": "electron-vite build && electron-builder --config electron-builder.json5 --linux",
    "test:e2e": "npm run build:electron && playwright test"
  },
