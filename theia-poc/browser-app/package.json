{
  "private": true,
  "name": "browser-app",
  "version": "0.0.0",
  "dependencies": {
    "@theia/core": "1.67.0",
    "@theia/editor": "1.67.0",
    "@theia/filesystem": "1.67.0",
    "@theia/markers": "1.67.0",
    "@theia/messages": "1.67.0",
    "@theia/monaco": "1.67.0",
    "@theia/navigator": "1.67.0",
    "@theia/preferences": "1.67.0",
    "@theia/process": "1.67.0",
    "@theia/terminal": "1.67.0",
    "@theia/workspace": "1.67.0",
    "writenow-theia-poc": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.67.0"
  },
  "scripts": {
    "bundle": "yarn rebuild && theia build --mode development",
    "rebuild": "theia rebuild:browser --cacheRoot .. --modules=node-pty,native-keymap,find-git-repositories,drivelist,keytar,ssh2,cpu-features,better-sqlite3",
    "start": "theia start",
    "watch": "yarn rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
